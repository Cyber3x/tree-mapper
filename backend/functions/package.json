{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve-clean": "firebase emulators:start",
    "serve": "firebase emulators:start --import=./dev-firestore --export-on-exit",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "busboy": "^0.3.1",
    "cors": "^2.8.5",
    "firebase": "^7.14.6",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
